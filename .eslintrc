{
  "extends": [
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "react/jsx-indent":0,
    "jsx-quotes":0,
    "react/jsx-tag-spacing":0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "semi": ["error", "never"],
    "comma-dangle": [2, "never"],
    "eol-last":0
  }
}
